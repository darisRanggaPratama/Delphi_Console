program Pinjaman;

uses
  Vcl.Forms,
  MainForm in 'MainForm.pas' {Home},
  Entry in 'Entry.pas' {formEntry};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(THome, Home);
  Application.CreateForm(TformEntry, formEntry);
  Application.Run;
end.
